<Application x:Class="Matrix.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Matrix"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             StartupUri="/View/Windows/MainContainer.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Style/MainStyle.xaml"/>
                <ResourceDictionary Source="/Style/IconsDictionary.xaml"/>
                <ui:ThemesDictionary Theme="Dark" />
                <ui:ControlsDictionary />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
